				float4 lerpResult10_g1 = lerp( temp_output_2_0_g1 , temp_cast_1 , _Gray);
				
				half4 color = lerpResult10_g1;
				
				#ifdef UNITY_UI_CLIP_RECT
                color.a *= UnityGet2DClipping(IN.worldPosition.xy, _ClipRect);
                #endif
				
				#ifdef UNITY_UI_ALPHACLIP
				clip (color.a - 0.001);
				#endif

				return color;
			}
		ENDCG
		}
